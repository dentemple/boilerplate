{
  "name": "dens-react-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "storybook-to-ghpages",
    "test": "react-scripts test --env=jsdom",
    "test:update": "react-scripts test --env=jsdom --updateSnapshot",
    "test:coverage": "react-scripts test --env=jsdom --coverage",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "history": "^4.7.2",
    "prop-types": "^15.6.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-icons": "^2.2.7",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-scripts": "1.0.17",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "styled-components": "^2.2.4",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.0-alpha.1",
    "@storybook/addon-links": "^3.4.0-alpha.1",
    "@storybook/react": "^3.4.0-alpha.1",
    "@storybook/storybook-deployer": "^2.3.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "husky": "^0.14.3",
    "jest-enzyme": "^4.0.1",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.1",
    "react-test-renderer": "^16.2.0",
    "redux-devtools-extension": "^2.13.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --no-semi --write",
      "git add"
    ]
  }
}
